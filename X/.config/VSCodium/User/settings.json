{
  "[markdown]": {
    "editor.tabSize": 4
  },
  "[html]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[xml]": {
    "editor.tabSize": 2
  },
  "[css]": {
    "editor.tabSize": 2
  },
  "[scss]": {
    "editor.tabSize": 2
  },
  "[sass]": {
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "editor.fontSize": 16,
  // "editor.fontFamily": "mononoki",
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": false,
  "liveServer.settings.donotVerifyTags": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "razor": "html",
    "plaintext": "pug"
  },
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "eslint.alwaysShowStatus": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "sqltools.useNodeRuntime": true,
  "redhat.telemetry.enabled": false,
  //"workbench.colorTheme": "BeardedTheme Black & Amethyst",
  "window.titleBarStyle": "custom",
  "todo-tree.tree.showScanModeButton": false,
  "java.home": "/usr/lib/jvm/java-16-adoptopenjdk",
  "java.jdt.ls.vmargs": "-Dfile.encoding=UTF-8",
  "files.autoGuessEncoding": true,
  "files.associations": {
    "*.mcmeta": "json"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "yaml.schemas": {
    "https://json.schemastore.org/traefik-v2-file-provider.json": "traefik/dynamic/*"
  },
  "codetogether.userName": "Lukas",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.smoothScrolling": true
}
